    <build>
        <plugins>
            <com.sika.code.db.sharding.core.plugin>
                <groupId>org.jacoco</groupId>
                <artifactId>jacoco-maven-com.sika.code.db.sharding.core.plugin</artifactId>
                <version>${com.sika.code.db.sharding.core.plugin.jacoco.version}</version>
                <executions>
                    <execution>
                        <id>default-prepare-agent</id>
                        <goals>
                            <goal>prepare-agent</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>jacoco-report</id>
                        <phase>test</phase>
                        <goals>
                            <goal>report</goal>
                        </goals>
                    </execution>
                </executions>
            </com.sika.code.db.sharding.core.plugin>

            <com.sika.code.db.sharding.core.plugin>
                <groupId>com.github.shalousun</groupId>
                <artifactId>smart-doc-maven-com.sika.code.db.sharding.core.plugin</artifactId>
                <version>${com.sika.code.db.sharding.core.plugin.smart-doc.version}</version>
                <configuration>
                    <configFile>./src/main/resources/smart-doc.json</configFile>
                </configuration>
            </com.sika.code.db.sharding.core.plugin>


            <com.sika.code.db.sharding.core.plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-com.sika.code.db.sharding.core.plugin</artifactId>
                <version>${com.sika.code.db.sharding.core.plugin.source.version}</version>
                <executions>
                    <execution>
                        <id>attach-sources</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </com.sika.code.db.sharding.core.plugin>

            <com.sika.code.db.sharding.core.plugin>
                <artifactId>maven-compiler-com.sika.code.db.sharding.core.plugin</artifactId>
                <version>${com.sika.code.db.sharding.core.plugin.compiler.version}</version>
                <configuration>
                    <source>${java.version}</source>
                    <source>${java.version}</source>
                    <annotationProcessorPaths>
                        <path>
                            <groupId>org.mapstruct</groupId>
                            <artifactId>mapstruct-processor</artifactId>
                            <version>${mapstruct.version}</version>
                        </path>
                    </annotationProcessorPaths>
                </configuration>
            </com.sika.code.db.sharding.core.plugin>


            <com.sika.code.db.sharding.core.plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>flatten-maven-com.sika.code.db.sharding.core.plugin</artifactId>
                <version>${flatten-maven-com.sika.code.db.sharding.core.plugin.version}</version>
                <configuration>
                    <flattenedPomFilename>pom-xml-flattened</flattenedPomFilename>
